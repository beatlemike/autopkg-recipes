<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Downloads the current release version of the Zoom client for IT, builds a package, then uploads to the JSS</string>
        <key>Identifier</key>
        <string>com.github.beatlemike.jss.zoom</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>Zoom Client for Meetings</string>
            <key>CATEGORY</key>
            <string>Communication</string>
            <key>POLICY_CATEGORY</key>
            <string>Communication</string>
            <key>POLICY_TRIGGER</key>
            <string>zoom</string>
            <key>POLICY_TEMPLATE</key>
            <string>%RECIPE_DIR%/PolicyTemplate.xml</string>
            <key>ICON</key>
            <string>%RECIPE_DIR%/ZoomIT.png</string>
            <key>DESCRIPTION</key>
            <string>The Zoom app provides cloud-hosted video conferencing, plus online meetings and group messaging.</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.2.0</string>
        <key>ParentRecipe</key>
        <string>com.github.smithjw.download.ZoomIT</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Arguments</key>
                <dict>
                    <key>prod_name</key>
                    <string>%NAME%</string>
                    <key>category</key>
                    <string>%CATEGORY%</string>
                    <key>policy_category</key>
                    <string>%POLICY_CATEGORY%</string>
                    <key>trigger_other</key>
                    <string>%POLICY_TRIGGER%</string>
                    <key>policy_template</key>
                    <string>%POLICY_TEMPLATE%</string>
                    <key>self_service_icon</key>
                    <string>%ICON%</string>
                    <key>self_service_description</key>
                    <string>%DESCRIPTION%</string>
                </dict>
                <key>Processor</key>
                <string>JSSImporter</string>
            </dict>
        </array>
    </dict>
</plist>
